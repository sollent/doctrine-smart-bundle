{
    "type": "symfony-bundle",
    "name": "sollent/doctrine-smart-bundle",
    "version": "1.0.0",
    "license": "proprietary",
    "require": {
        "php": ">=7.2.5",
        "nette/php-generator": "^3.6",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.10",
        "symfony/asset": "5.3.*",
        "symfony/form": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/yaml": "5.3.*",
        "twig/extra-bundle": "^2.12|^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
